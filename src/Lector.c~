#define light_blue "\033[1;34m"
#define violet "\033[0;35m"
#define light_violet "\033[1;35m"
#define cyan "\033[0;36m"
#define light_cyan "\033[1;36m"
#define light_grey "\033[0;37m"
#define white "\033[1;37m"

